@model IEnumerable<WardrobeWeb.Models.Outfit>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OutfitName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TopID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OutfitSeason)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OutfitOccasion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Accessory.AccessName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Bottom.BottomName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sho.ShoeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Top.TopName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.OutfitName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TopID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OutfitSeason)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OutfitOccasion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Accessory.AccessName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Bottom.BottomName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sho.ShoeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Top.TopName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.OutfitsID }) |
            @Html.ActionLink("Details", "Details", new { id=item.OutfitsID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.OutfitsID })
        </td>
    </tr>
}

</table>
