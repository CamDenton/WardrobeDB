@model IEnumerable<WardrobeWeb.Models.Accessory>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>


    @foreach (var item in Model)
    {
        <row>
            <div class="col-xs-12, col-sm-12, col-md-6, col-lg-3">
               <h3> @Html.DisplayNameFor(model => model.AccessName)</h3>
                @Html.DisplayFor(modelItem => item.AccessName)
                </div>
                
 
                     <div class="col-xs-12, col-sm-12, col-md-6, col-lg-3">
                         <h3>
                             @Html.DisplayNameFor(model => model.AccessType)</h3>
                         @Html.DisplayFor(modelItem => item.AccessType)
</div>
                        
                         <div class="col-xs-12, col-sm-12, col-md-6, col-lg-3">
                             
                             <h3>
                                 @Html.DisplayNameFor(model => model.AccessSeason)</h3>
                             @Html.DisplayFor(modelItem => item.AccessSeason)
</div>
                          
                             <div class="col-xs-12, col-sm-12, col-md-6, col-lg-3">
                                 <h3>
                                     @Html.DisplayNameFor(model => model.AccessOccasion)</h3>
                                     @Html.DisplayFor(modelItem => item.AccessOccasion)
</div>
                        
                                 <div class="col-xs-12, col-sm-12, col-md-6, col-lg-3">
                                     @Html.ActionLink("Edit", "Edit", new { id = item.AccessID }) |
                                     @Html.ActionLink("Details", "Details", new { id = item.AccessID }) |
                                     @Html.ActionLink("Delete", "Delete", new { id = item.AccessID })
                                     </div>
            </row>
  
    }

                           